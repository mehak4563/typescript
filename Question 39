
function cityCountry(city: string, country: string): string {
    return `${city}, ${country}`;
}

let result1: string = cityCountry('Lahore', 'Pakistan');
let result2: string = cityCountry('Paris', 'France');
let result3: string = cityCountry('New York', 'USA');

console.log(result1);
console.log(result2);
console.log(result3);
